<!-- Imported from PyPoE -->
<format file="NPCs">
    <field name="Id"                    type="string" unique="true" /> <!-- file_path=True -->
    <field name="Name"                  type="string" />
    <field name="Metadata"              type="string" /> <!-- file_path=True -->
    <field name="Unknown0"              type="i32" />
    <field name="NPCMasterKey"          type="longid" ref="NPCMaster" />
    <field name="ShortName"             type="string" />
    <field name="Unknown1"              type="i32" />
    <field name="NPCShopKey"            type="longid" ref="NPCShop" />
    <field name="GreetingNPCAudioKeys"  type="longid[]" ref="NPCAudio" />
    <field name="GoodbyeNPCAudioKeys"   type="longid[]" ref="NPCAudio" />
    <field name="Unknown2"              type="i32" /> <!-- unique and under 65536 -->
    <field name="Unknown3"              type="shortid" since="3.11" /> <!-- unused until 3.15 -->
    <field name="PortraitFile"          type="i32" since="3.12" until="3.14" /> <!-- file_path=True -->
    <field name="NPCPortraitsKey"       type="longid" ref="NPCPortraits" since="3.13" />
    <field name="Unknown5"              type="longid" since="3.14" />
</format>
