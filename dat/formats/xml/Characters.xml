<!-- Imported from PyPoE -->
<format file="Characters">
    <field name="Id"                    type="string" unique="true" />
    <field name="Name"                  type="string" unique="true" />
    <field name="AOFile"                type="string" /> <!-- file_path=True file_ext=.ao -->
    <field name="ACTFile"               type="string" /> <!-- file_path=True file_ext=.act -->
    <field name="BaseMaxLife"           type="i32" />
    <field name="BaseMaxMana"           type="i32" />
    <field name="WeaponSpeed"           type="i32" /> <!-- Attack Speed in milliseconds -->
    <field name="MinDamage"             type="i32" />
    <field name="MaxDamage"             type="i32" />
    <field name="MaxAttackDistance"     type="i32" />
    <field name="Icon"                  type="string" />
    <field name="IntegerId"             type="i32" unique="true" />
    <field name="BaseStrength"          type="i32" />
    <field name="BaseDexterity"         type="i32" />
    <field name="BaseIntelligence"      type="i32" />
    <field name="Keys0"                 type="u64[]" />
    <field name="Description"           type="string" />
    <field name="StartSkillGem_SkillGemsKey" type="r64" ref="SkillGems" />
    <field name="Unknown13"             type="i32" />
    <field name="Unknown14"             type="i32" />
    <field name="Unknown15"             type="i32" />
    <field name="Unknown16"             type="i32" />
    <field name="CharacterSize"         type="i32" />
    <field name="IntroSoundFile"        type="string" /> <!-- file_path=True file_ext=.ogg -->
    <field name="StartWeapon_BaseItemTypesKey" type="r64" ref="BaseItemTypes" />
    <field name="Unknown27"             type="r32" />
    <field name="TraitDescription"      type="string" />
    <field name="Key0"                  type="u64" />
    <field name="Key1"                  type="u64" />
    <field name="Key2"                  type="u64" />
    <field name="Key3"                  type="u64" />
    <field name="Unknown31"             type="i32" />
    <field name="Keys1"                 type="u64[]" />
</format>
