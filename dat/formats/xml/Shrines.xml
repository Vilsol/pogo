    <field name="TimeoutInSeconds"      type="i32" />
    <field name="Name"                  type="string" />
    <field name="ChargesShared"         type="bool" /> <!-- display=ChargesShared? -->
    <field name="Player_ShrineBuffsKey" type="longid" ref="ShrineBuffs" />
    <field name="Unknown4"              type="i32" />
    <field name="Unknown5"              type="i32" />
