    <field name="Monster_ShrineBuffsKey" type="longid" ref="ShrineBuffs" />
    <field name="SummonMonster_MonsterVarietiesKey" type="longid" ref="MonsterVarieties" /> <!-- The aoe ground effects for example -->
    <field name="SummonPlayer_MonsterVarietiesKey" type="longid" ref="MonsterVarieties" /> <!-- The aoe ground effects for example -->
    <field name="Unknown10"             type="i32" />
    <field name="Unknown11"             type="i32" />
    <field name="ShrineSoundsKey"       type="longid" ref="ShrineSounds" />
    <field name="Unknown14"             type="bool" />
    <field name="AchievementItemsKeys"  type="longid[]" ref="AchievementItems" />
    <field name="IsPVPOnly"             type="bool" />
    <field name="Unknown17"             type="bool" />
