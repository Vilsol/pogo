<!-- Imported from PyPoE -->
<format file="Prophecies">
    <field name="Id"                    type="string" unique="true" />
    <field name="PredictionText"        type="string" />
    <field name="UnknownUnique"         type="i32" unique="true" />
    <field name="Name"                  type="string" />
    <field name="FlavourText"           type="string" />
    <field name="QuestTracker_ClientStringsKeys" type="r64[]" ref="ClientStrings" />
    <field name="OGGFile"               type="string" /> <!-- file_path=True file_ext=.ogg -->
    <field name="ProphecyChainKey"      type="r64" ref="ProphecyChain" />
    <field name="ProphecyChainPosition" type="i32" />
    <field name="IsEnabled"             type="bool" />
    <field name="SealCost"              type="i32" />
    <field name="PredictionText2"       type="string" />
</format>
