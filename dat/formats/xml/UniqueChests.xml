<!-- Imported from PyPoE -->
<format file="UniqueChests">
    <field name="Id"                    type="string" unique="true" />
    <field name="WordsKey"              type="r64" ref="Words" />
    <field name="FlavourTextKey"        type="r64" ref="FlavourText" />
    <field name="MinLevel"              type="i32" />
    <field name="ModsKeys"              type="r64[]" ref="Mods" />
    <field name="SpawnWeight"           type="i32" />
    <field name="Data1"                 type="i32[]" />
    <field name="AOFile"                type="string" /> <!-- file_path=True file_ext=.ao -->
    <field name="Flag0"                 type="bool" />
    <field name="Data2"                 type="u32[]" />
    <field name="AppearanceChestsKey"   type="r64" ref="Chests" /> <!-- Uses this chest for it"s visuals -->
    <field name="ChestsKey"             type="r64" ref="Chests" />
    <field name="Keys0"                 type="u64[]" />
</format>
