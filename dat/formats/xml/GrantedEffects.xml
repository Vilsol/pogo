<!-- Imported from PyPoE -->
<format file="GrantedEffects">
    <field name="Id"                    type="string" unique="true" />
    <field name="IsSupport"             type="bool" />
    <field name="AllowedActiveSkillTypes" type="u32[]" /> <!-- This support gem only supports active skills with at least one of these types -->
    <field name="BaseEffectiveness"     type="f32" />
    <field name="IncrementalEffectiveness" type="f32" />
    <field name="SupportGemLetter"      type="string" />
    <field name="Unknown0"              type="i32" />
    <field name="AddedActiveSkillTypes" type="u32[]" /> <!-- This support gem adds these types to supported active skills -->
    <field name="ExcludedActiveSkillTypes" type="u32[]" /> <!-- This support gem does not support active skills with one of these types -->
    <field name="SupportsGemsOnly"      type="bool" /> <!-- This support gem only supports active skills that come from gem items -->
    <field name="Unknown1"              type="u32" />
    <field name="Data3"                 type="i32[]" />
    <field name="Flag1"                 type="bool" />
    <field name="Unknown2"              type="i32" />
    <field name="CastTime"              type="i32" />
    <field name="ActiveSkillsKey"       type="longid" ref="ActiveSkills" />
    <field name="Flag2"                 type="bool" />
    <field name="Flag3"                 type="bool" />
    <field name="Data4"                 type="i32[]" />
    <field name="Key0"                  type="u64" />
    <field name="Key1"                  type="u64" />
    <field name="Flag4"                 type="bool" />
    <field name="Keys0"                 type="u64[]" />
    <field name="GrantedEffectsKey"     type="shortid" ref="GrantedEffects" />
</format>
