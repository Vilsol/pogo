<!-- Imported from PyPoE -->
<format file="MapPins">
    <field name="Id"                    type="string" unique="true" />
    <field name="PositionX"             type="i32" /> <!-- X starts at left side of image, can be negative -->
    <field name="PositionY"             type="i32" /> <!-- Y starts at top side of image, can be negative -->
    <field name="Waypoint_WorldAreasKey" type="longid" ref="WorldAreas" />
    <field name="WorldAreasKeys"        type="longid[]" ref="WorldAreas" />
    <field name="Name"                  type="string" />
    <field name="FlavourText"           type="string" />
    <field name="Data1"                 type="u32[]" />
    <field name="Act"                   type="i32" />
    <field name="UnknownId"             type="string" />
    <field name="Data0"                 type="i32[]" />
    <field name="Unknown13"             type="i32" />
</format>
