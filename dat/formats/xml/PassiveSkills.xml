<!-- Imported from PyPoE -->
<format file="PassiveSkills">
    <field name="Id"                    type="string" unique="true" />
    <field name="Icon_DDSFile"          type="string" /> <!-- file_path=True file_ext=.dds -->
    <field name="StatsKeys"             type="longid[]" ref="Stats" />
    <field name="Stat1Value"            type="i32" />
    <field name="Stat2Value"            type="i32" />
    <field name="Stat3Value"            type="i32" />
    <field name="Stat4Value"            type="i32" />
    <field name="PassiveSkillGraphId"   type="i32" unique="true" /> <!-- Id used by PassiveSkillGraph.psg -->
    <field name="Name"                  type="string" />
    <field name="CharactersKeys"        type="longid[]" ref="Characters" />
    <field name="IsKeystone"            type="bool" />
    <field name="IsNotable"             type="bool" />
    <field name="FlavourText"           type="string" />
    <field name="IsJustIcon"            type="bool" />
    <field name="AchievementItemsKey"   type="longid" ref="AchievementItems" />
    <field name="IsJewelSocket"         type="bool" />
    <field name="AscendancyKey"         type="longid" ref="Ascendancy" />
    <field name="IsAscendancyStartingNode" type="bool" />
    <field name="Reminder_ClientStringsKeys" type="longid[]" ref="ClientStrings" />
    <field name="SkillPointsGranted"    type="i32" />
    <field name="IsMultipleChoice"      type="bool" />
    <field name="IsMultipleChoiceOption" type="bool" />
    <field name="Stat5Value"            type="i32" />
    <field name="PassiveSkillBuffsKeys" type="longid[]" ref="PassiveSkillBuffs" />
    <field name="GrantedEffectsPerLevelKey" type="longid" ref="GrantedEffectsPerLevel" />
    <field name="Flag0"                 type="bool" />
    <field name="Unknown0"              type="i32" />
    <field name="Flag1"                 type="u8" />
    <field name="IsProxyPassive"        type="u8" />
    <field name="Unknown1"              type="i32" />
</format>
